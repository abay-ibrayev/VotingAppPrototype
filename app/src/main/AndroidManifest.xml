<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shareholder.abay.finapps.finappsproject">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/logos"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".splashscreen.SplashScreenActivity">

        </activity>
        <activity android:name=".splashscreen.SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.RecentVotesActivity" />
        <activity android:name=".activity.HistoryActivity" />
        <activity android:name=".activity.MessagesActivity" />
        <activity android:name=".activity.ProfileActivity" />
        <activity android:name=".activity.VoteActivity" />
        <activity android:name=".activity.DecisionActivity" />
        <activity android:name=".activity.HistoryDescActivity" />
        <activity android:name=".activity.ArchiveMessagesActivity"></activity>
    </application>

</manifest>